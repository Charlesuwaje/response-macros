{
  "name": "charlesuwaje/response-macros",
  "description": "Standard API response macros for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Uwaje",
      "email": "charlesuwaje02@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
    "phpunit/phpunit": "^9.0 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "CharlesUwaje\\ResponseMacros\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CharlesUwaje\\ResponseMacros\\Providers\\ResponseMacrosServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
